    "version": "0.2.0",
    "configurations": [
        {
            "name": "Streamlit",
            "type": "debugpy",
            "request": "launch",
            "module": "streamlit",
            "args": ["run", "frontend/app.py"],
            "console": "integratedTerminal",
            "env": {
                "STREAMLIT_SERVER_PORT": "8501",
                "STREAMLIT_SERVER_HEADLESS": "true",
                "STREAMLIT_SERVER_ENABLECORS": "false"
            }
        }
    ]
}